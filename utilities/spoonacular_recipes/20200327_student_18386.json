ts": [], "occasions": [], "winePairing": {"pairedWines": ["chianti", "trebbiano", "verdicchio"], "pairingText": "Chianti, Trebbiano, and Verdicchio are my top picks for Gnocchi. Italians know food and they know wine. Trebbiano and Verdicchio are Italian white wines that pair well with fish and white meat, while Chianti is a great Italian red for heavier, bolder dishes. You could try Bibbiano Vigna del Capannino Chianti Classico Grand Selezione. Reviewers quite like it with a 4.1 out of 5 star rating and a price of about 34 dollars per bottle.", "productMatches": [{"id": 454138, "title": "Bibbiano Vigna del Capannino Chianti Classico Grand Selezione", "description": "Very intense and complex, with soft fruity notes of black cherry and cherry, together with intense and pleasant spicy scents, particularly of tobacco, cocoa and black pepper mingled with agreable balsamic and mineral notes.", "price": "$33.99", "imageUrl": "https://spoonacular.com/productImages/454138-312x231.jpg", "averageRating": 0.82, "ratingCount": 9.0, "score": 0.7842857142857143, "link": "https://click.linksynergy.com/deeplink?id=*QCiIS6t4gA&mid=2025&murl=https%3A%2F%2Fwww.wine.com%2Fproduct%2Fbibbiano-vigna-del-capannino-chianti-classico-grand-selezione-2011%2F203342"}]}, "instructions": null, "analyzedInstructions": [], "originalId": null}]