tle.", "productMatches": [{"id": 449768, "title": "Rex Hill Willamette Valley Pinot Noir", "description": "Aromas of violets, iris, ripe raspberries, dark cherries, rose, blueberries, wild strawberries and wet stones all emerge from the glass when the wine is first poured. As the wine opens, more savory aromas of wood smoke, cured tobacco, earth, sandalwood and cinnamon add complexity to the original aromatics. Structured, ripe and pure, the wine shows all hallmarks of the 2013 vintage.", "price": "$28.99", "imageUrl": "https://spoonacular.com/productImages/449768-312x231.jpg", "averageRating": 0.9199999999999999, "ratingCount": 10.0, "score": 0.8877419354838709, "link": "https://click.linksynergy.com/deeplink?id=*QCiIS6t4gA&mid=2025&murl=https%3A%2F%2Fwww.wine.com%2Fproduct%2Frex-hill-willamette-valley-pinot-noir-2013%2F150589"}]}, "instructions": null, "analyzedInstructions": [], "sourceName": null, "creditsText": null, "originalId": null}]