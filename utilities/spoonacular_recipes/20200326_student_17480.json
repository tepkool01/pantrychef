.", "productMatches": [{"id": 473408, "title": "Cupcake Vineyards Butterkissed Chardonnay", "description": "The Butterkissed Chardonnay is a rich, decadent take on a timeless varietal. Cupcake Vineyards grows its grapes in the sun-drenched vineyards of California, where the long, warm growing season helps coax rich, indulgent flavors into the bottle. Butterkissed Chardonnay has a smooth, creamy mouthfeel, with luscious buttery notes and hints of vanilla, white peach and citrus zest. Serve chilled with wood-fired pizza, parmesan crusted chicken breast or at impromptu get-togethers with friends.", "price": "$10.99", "imageUrl": "https://spoonacular.com/productImages/473408-312x231.jpg", "averageRating": 0.8, "ratingCount": 25.0, "score": 0.7868421052631579, "link": "https://click.linksynergy.com/deeplink?id=*QCiIS6t4gA&mid=2025&murl=https%3A%2F%2Fwww.wine.com%2Fproduct%2Fcupcake-vineyards-butterkissed-chardonnay-2017%2F509991"}]}, "instructions": null, "analyzedInstructions": [], "originalId": null}]