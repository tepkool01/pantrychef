occasions": [], "winePairing": {"pairedWines": ["merlot", "pinot noir", "rose wine"], "pairingText": "Tuna works really well with Merlot, Pinot Noir, and ros\u00e9 Wine. Though fish is often paired with white wine, 'meatier' fish like tuna can absolutely go with red wine. A Ros\u00e9 will also pair nicely, particularly if your tunan is prepared with ingredients better suited to a white wine. One wine you could try is Clos Du Val Merlot. It has 4.1 out of 5 stars and a bottle costs about 30 dollars.", "productMatches": [{"id": 433037, "title": "Clos Du Val Merlot", "description": "A dense, inky color and rich aromas of intense black currant and blackberry pervade our 2002 Merlot. Hints of herbs, black olives and toast are also present in the nose, leading to a well-structured wine with an abundance of dark red and black fruit flavors, balanced with sweet toast and some spice. The wine's soft tannins, lingering finish, and rich, complex taste make this a delightful accompaniment to roast chicken, pork, or beef.", "price": "$29.99", "imageUrl": "https://spoonacular.com/productImages/433037-312x231.jpg", "averageRating": 0.82, "ratingCount": 9.0, "score": 0.7842857142857143, "link": "https://click.linksynergy.com/deeplink?id=*QCiIS6t4gA&mid=2025&murl=https%3A%2F%2Fwww.wine.com%2Fproduct%2Fclos-du-val-merlot-2002%2F83430"}]}, "instructions": null, "analyzedInstructions": [], "originalId": null}]